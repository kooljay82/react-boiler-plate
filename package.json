{
  "name": "react-boiler-plate",
  "version": "2.0.0",
  "description": "리액트 프로젝트 보일러 플레이트",
  "private": true,
  "engines": {
    "yarn": "use npm, not yarn"
  },
  "scripts": {
    "start": "webpack serve --config webpack.dev.js --env development",
    "build:dev": "webpack --config webpack.prod.js --env development",
    "build:stg": "webpack --config webpack.prod.js --env staging",
    "build": "webpack --config webpack.prod.js --env production",
    "docs": "npx typedoc",
    "docs:check": "node helperServer.ts docs",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "pret": "prettier --write \"src/\"",
    "pret:watch": "onchange \"src/\" -- prettier --write --ignore-unknown {{changed}}",
    "test:dev": "jest --watch",
    "test:unit": "jest",
    "test:check": "node helperServer.ts coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kooljay82/react-boiler-plate"
  },
  "bugs": {
    "url": "https://github.com/kooljay82/react-boiler-plate/issues",
    "email": "jinho82@gmail.com"
  },
  "keywords": [
    "react",
    "webpack5",
    "react boiler plate"
  ],
  "author": "임진호 <jinho82@gmail.com>",
  "homepage": "https://github.com/kooljay82/react-boiler-plate#readme",
  "overrides": {
    "xml2js": "0.5.0",
    "semver": "7.5.2",
    "send": "0.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.20.1",
    "css-loader": "^6.5.1",
    "csv-loader": "^3.0.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "jest-cli": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-svg-transformer": "^1.0.0",
    "jsdom": "^20.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.80.3",
    "sass-loader": "^16.0.2",
    "style-loader": "^3.3.1",
    "typedoc": "^0.23.7",
    "typescript": "^4.7.4",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@babel/runtime-corejs3": "^7.16.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reset-css": "^5.0.1",
    "web-vitals": "^2.1.4"
  }
}
