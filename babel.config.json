{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "defaults",
        "bugfixes": true,
        "shippedProposals": true
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "babel-plugin-transform-import-meta",
    "@babel/plugin-proposal-optional-chaining"
  ]
}
